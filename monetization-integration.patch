+  // Public methods called from GamePage
+  void startPath(Vector2 position, int color) {
+    print('üéØ Starting new path at position $position with color $color');
+    
+    // Reset current path
+    currentPath.clear();
+    currentColor = color;
+    
+    // Add first segment (start point)
+    currentPath.add(PathSegment(
