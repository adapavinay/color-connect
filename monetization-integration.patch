+      if (y.isEven) {
+        // Left to right
+        for (int x = 0; x < gridSize; x++) {
+          order.add(Pos(x, y));
+        }
+      } else {
+        // Right to left
+        for (int x = gridSize - 1; x >= 0; x--) {
+          order.add(Pos(x, y));
+        }
+      }
+    }
+    
+    return order;
+  }
